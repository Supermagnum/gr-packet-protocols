<?xml version="1.0"?>
<block>
  <name>FX.25 Encoder</name>
  <key>packet_protocols_fx25_encoder</key>
  <category>Packet Protocols</category>
  <import>from gnuradio import packet_protocols</import>
  <make>packet_protocols.fx25_encoder($fec_type, $interleaver_depth, $add_checksum)</make>
  <param>
    <name>FEC Type</name>
    <key>fec_type</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>Interleaver Depth</name>
    <key>interleaver_depth</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Add Checksum</name>
    <key>add_checksum</key>
    <value>True</value>
    <type>bool</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>





