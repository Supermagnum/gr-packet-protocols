<?xml version="1.0"?>
<block>
  <name>AX.25 Encoder</name>
  <key>packet_protocols_ax25_encoder</key>
  <category>Packet Protocols</category>
  <import>from gnuradio import packet_protocols</import>
  <make>packet_protocols.ax25_encoder($dest_callsign, $dest_ssid, $src_callsign, $src_ssid, $digipeaters, $command_response, $poll_final)</make>
  <param>
    <name>Destination Callsign</name>
    <key>dest_callsign</key>
    <value>N0CALL</value>
    <type>string</type>
  </param>
  <param>
    <name>Destination SSID</name>
    <key>dest_ssid</key>
    <value>0</value>
    <type>string</type>
  </param>
  <param>
    <name>Source Callsign</name>
    <key>src_callsign</key>
    <value>N0CALL</value>
    <type>string</type>
  </param>
  <param>
    <name>Source SSID</name>
    <key>src_ssid</key>
    <value>0</value>
    <type>string</type>
  </param>
  <param>
    <name>Digipeaters</name>
    <key>digipeaters</key>
    <value></value>
    <type>string</type>
  </param>
  <param>
    <name>Command/Response</name>
    <key>command_response</key>
    <value>False</value>
    <type>bool</type>
  </param>
  <param>
    <name>Poll/Final</name>
    <key>poll_final</key>
    <value>False</value>
    <type>bool</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>





