- [packet_protocols_kiss_tnc_0, '0', blocks_null_sink_0, '0']
- [packet_protocols_link_quality_monitor_0, '0', packet_protocols_adaptive_modulator_0,
  '0']

metadata:
  file_format: 1
  grc_version: 3.10.12.0
